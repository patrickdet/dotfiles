" Colorscheme
set t_Co=256
colorscheme molokai
set cursorline                    " Highlight the current line of the cursor
set colorcolumn=81                " to help us keep our lines under 80 chars

" Enable fancy mode 
let g:Powerline_symbols = 'fancy' " Powerline

" Wrap lines if line too long
set wrap

" NGINX
au BufRead,BufNewFile *nginx* set filetype=nginx
